<% include head %>
  <body>
    <div class='container'>
      <div class='row'>
        <div class='col col-12'>
          <h1>MOVIES NOW PLAYING</h1>
        </div>
          <% parseData.forEach((movie,i)=>{ %>
            <div class='col col-12 col-lg-3 col-md-4 col-sm-6 poster'>
                <a href='/movie/<%=movie.id%>'>
                  <img src='<%= imageBaseUrl + movie.poster_path%>'>
                </a>
            </div>
          <% })%>  
      </div>
    </div>    
  </body>
</html>
